/**
 * Implements hook_menu().
 */
function goods_menu()
{
    $items['?q=admin/products'] = array(
        'title' => t('产品管理'),
        'page callback' => 'goods_page_callback',
        'access arguments' => array('access content'),
        'type' => MENU_SUGGESTED_ITEM,
        'file' => 'goods.pages.inc',
    );

    return $items;
}